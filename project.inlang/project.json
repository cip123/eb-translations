{
  "$schema": "https://inlang.com/schema/project.json",
  "languageTags": [
    "en",
    "fr"
  ],
  "modules": [
    "https://cdn.jsdelivr.net/npm/@inlang/plugin-json@latest"
  ],
  "plugin.inlang.json": {
    "pathPattern": {
      "definitions": "locales/{languageTag}/definitions.json",
      "translations": "locales/{languageTag}/translations.json"
    }
  },
  "sourceLanguageTag": "en"
}